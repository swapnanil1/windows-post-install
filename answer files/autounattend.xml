            </OOBE>
            <UserAccounts>
                <AdministratorPassword>
                    <Value>pacman</Value>
                    <PlainText>true</PlainText>
                </AdministratorPassword>
                <LocalAccounts>
                    <LocalAccount wcm:action="add">
                        <Password>
                            <Value></Value>
                            <PlainText>true</PlainText>
                        </Password>
                        <Description></Description>
                        <DisplayName>Swapnanil</DisplayName>
                        <Group>Users</Group>
                        <Name>Swapnanil</Name>
